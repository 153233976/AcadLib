<wpf:BaseUserControl x:Class="AcadLib.PaletteProps.IntListValueView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:NetLib.WPF;assembly=NetLib"
        xmlns:paletteProps="clr-namespace:AcadLib.PaletteProps"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
        d:DataContext="{d:DesignInstance paletteProps:IntListValueVM}"
        mc:Ignorable="d">
    <ComboBox ItemsSource="{Binding Values}" Text="{Binding Value}" IsEditable="{Binding AllowCustomValue}"
              IsReadOnly="{Binding IsReadOnly}">
    </ComboBox>
</wpf:BaseUserControl>
