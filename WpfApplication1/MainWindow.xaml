<wpf1:BaseWindow 
    x:Class="WpfApplication1.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:WpfApplication1"
    xmlns:wpf="clr-namespace:AcadLib.WPF;assembly=AcadLib"
    xmlns:wpf1="clr-namespace:NetLib.WPF;assembly=NetLib"
    xmlns:controls="clr-namespace:AcadLib.WPF.Controls;assembly=AcadLib"
    xmlns:converters="clr-namespace:AcadLib.WPF.Converters;assembly=AcadLib"
    d:DataContext="{d:DesignInstance local:Model, d:IsDesignTimeCreatable=True}"
    mc:Ignorable="d"
    Title="MainWindow" Height="68.173" Width="170.783">
    <StackPanel>
		 <controls:TransparenceSlider Transparence="{Binding Transparence, UpdateSourceTrigger=PropertyChanged}"/>
		<TextBlock Text="{Binding Transparence}"/>
		<BulletDecorator></BulletDecorator>
		<Button Command="{Binding ShowDialog}" Content="Dialog"/>
    </StackPanel>
</wpf1:BaseWindow>